{
  "$id": "inline_block.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["block_name", "realization_name"],
  "additionalProperties": false,

  "properties": {
    "block_name": {
      "type": "string",
      "minLength": 1,
      "description": "Name of nested block"
    },
    "realization_name": {
      "type": "string",
      "minLength": 1,
      "description": "Realization name of nested block"
    }
  }
}

